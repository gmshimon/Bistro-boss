{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/Models/Menu/(.*)",
            "dest": "./Models/Menu/menu.route.js"
        },
        {
            "src": "./Models/Reviews/(.*)",
            "dest": "./Models/Reviews/review.route.js"
        },
        {
            "src": "./Models/User/(.*)",
            "dest": "./Models/User/user.route.js"
        }
    ]
}
